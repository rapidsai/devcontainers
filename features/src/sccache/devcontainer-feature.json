{
  "name": "sccache",
  "id": "sccache",
  "version": "25.12.0",
  "description": "A feature to install sccache",
  "options": {
    "repository": {
      "type": "string",
      "proposals": [
        "mozilla/sccache"
      ],
      "default": "mozilla/sccache",
      "description": "The GitHub repository from which to install the sccache binary."
    },
    "version": {
      "type": "string",
      "proposals": [
        "latest",
        "dev",
        "0.7.7",
        "0.4.0",
        "0.3.1",
        "0.3.0",
        "0.2.15"
      ],
      "default": "latest",
      "description": "sccache version to install."
    }
  },
  "containerEnv": {
    "BASH_ENV": "/etc/bash.bash_env"
  }
}
