{
  "name": "NVIDIA CCCL development utilities",
  "id": "cccl-dev",
  "version": "25.4.0",
  "description": "A feature to install NVIDIA CCCL development utilities",
  "options": {
    "litVersion": {
      "type": "string",
      "proposals": [
        "latest",
        "16.0.2"
      ],
      "default": "latest",
      "description": "lit version to install."
    }
  },
  "mounts": [
    {
      "type": "cache",
      "target": "/var/tmp"
    },
    {
      "type": "cache",
      "target": "/var/cache/apt"
    },
    {
      "type": "cache",
      "target": "/var/lib/apt"
    }
  ]
}
