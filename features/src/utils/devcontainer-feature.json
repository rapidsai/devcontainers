{
  "name": "devcontainer utils",
  "id": "utils",
  "version": "23.8.0",
  "description": "A feature to install my devcontainer utilities",
  "containerEnv": {
    "BASH_ENV": "/etc/bash.bash_env"
  },
  "postCreateCommand": [
    "cron"
  ],
  "postAttachCommand": [
    "/bin/bash",
    "-li",
    "+o",
    "history",
    "devcontainer-utils-post-attach-command"
  ]
}
