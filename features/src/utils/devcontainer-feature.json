{
  "name": "devcontainer-utils",
  "id": "utils",
  "version": "25.4.1",
  "description": "A feature to install RAPIDS devcontainer utility scripts",
  "containerEnv": {
    "BASH_ENV": "/etc/bash.bash_env"
  },
  "postCreateCommand": [
    "/bin/bash",
    "-l",
    "+o",
    "history",
    "devcontainer-utils-post-create-command"
  ],
  "postStartCommand": [
    "/bin/bash",
    "-l",
    "+o",
    "history",
    "devcontainer-utils-post-start-command"
  ],
  "postAttachCommand": [
    "/bin/bash",
    "-l",
    "+o",
    "history",
    "devcontainer-utils-post-attach-command"
  ]
}
